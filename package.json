{
  "name": "portfoliobackend",
  "version": "1.0.0",
  "description": "Backend for handle portfolio data",
  "main": "server.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/server.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/server.js\"",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./typeOrm.config.ts",
    "typeorm:generate-migration": "npm run typeorm -- -d ./typeOrm.config.ts migration:generate ./migrations/$npm_config_name",
    "typeorm:create-migration": "npm run typeorm -- migration:create ./migrations/$npm_config_name",
    "typeorm:revert-migration": "npm run typeorm -- -d ./typeOrm.config.ts migration:revert"
  },
  "author": "ElvisDev",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@sendgrid/mail": "^7.7.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "connect-pg-simple": "^8.0.0",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "express-session": "^1.17.3",
    "imagekit": "^4.1.1",
    "jwks-rsa": "^3.1.0",
    "mailgun.js": "^9.3.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/connect-pg-simple": "^7.0.3",
    "@types/cors": "^2.8.12",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.19.3",
    "@types/pg": "^8.6.5",
    "concurrently": "^7.5.0",
    "nodemon": "^2.0.20",
    "prisma": "^5.7.0",
    "sequelize-cli": "^6.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.3"
  }
}
